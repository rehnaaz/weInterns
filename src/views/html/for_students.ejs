<body>

  <%- include('header.html') %>

    <div id="internships" class="container-fluid section">
      <div class="max-width-container">
        <h2>Internships</h2>
        <p class="section_description">
          Apply to 10,000+ internships for free
          <a class="view_all_cta" href="internships.ejs">
            View all internships <i class="ic-24-arrow-right"></i>
          </a>
        </p>

        <!-- First Main Div    -->
        <div class="categories_container container my-5">
          <h2 class="text-center mb-4">Certification Courses for You</h2>

          <!-- Inside the 8 div    -->
          <div class="grid">
            <!-- Card 1 -->
            <div class="card">
              <img src="/static/images/java.png.webp" alt="Core Java">
              <div class="card-content">
                <div class="card-title">Core Java</div>
                <div class="card-text">8 weeks • ⭐ 4.3 • 27,728 learners</div>
              </div>
            </div>
            <!-- Card 2 -->
            <div class="card">
              <img src="/static/images/web-development.png.webp" alt="Web Development">
              <div class="card-content">
                <div class="card-title">Web Development</div>
                <div class="card-text">8 weeks • ⭐ 4.1</div>
              </div>
            </div>
            <!-- Card 3 -->
            <div class="card">
              <img src="/static/images/android.png.webp" alt="Android App">
              <div class="card-content">
                <div class="card-title">Android App Development</div>
                <div class="card-text">8 weeks • ⭐ 4.1 • 38,450 learners</div>
              </div>
            </div>
            <!-- Card 4 -->
            <div class="card">
              <img src="/static/images/machine-learning.png.webp" alt="Machine Learning">
              <div class="card-content">
                <div class="card-title">Machine Learning</div>
                <div class="card-text">8 weeks • ⭐ 4.5 • 36,266 learners</div>
              </div>
            </div>
          </div>
        </div>

        <!-- Second Main Div -->
        <div class="categories_container container my-5">
          <h2 class="text-center mb-4">Certification Courses for You</h2>
          <div class="grid">
            <!-- Card 1 -->
            <div class="card">
              <img src="/static/images/" alt="Placement Guarantee">
              <div class="card-content">
                <div class="card-title">Placement Guarantee Courses</div>
                <div class="card-text">Full refund if not hired, confirmed salary up to 5 LPA.</div>
              </div>
            </div>
            <!-- Card 2 -->
            <div class="card">
              <img src="https://via.placeholder.com/300x150" alt="Gaming Mapping">
              <div class="card-content">
                <div class="card-title">Gaming Mapping Internship</div>
                <div class="card-text">Work From Home • ₹8,000/month • 6 Months</div>
              </div>
            </div>
            <!-- Card 3 -->
            <div class="card">
              <img src="https://via.placeholder.com/300x150" alt="Flutter Engineer">
              <div class="card-content">
                <div class="card-title">Software Engineer (Flutter)</div>
                <div class="card-text">₹20,00,000 - 50,00,000/year • Work From Home</div>
              </div>
            </div>
            <!-- Card 4 -->
            <div class="card">
              <img src="https://via.placeholder.com/300x150" alt=".NET Developer">
              <div class="card-content">
                <div class="card-title">Senior .NET Developer</div>
                <div class="card-text">₹9,00,000 - 10,00,000/year • Work From Home</div>
              </div>
            </div>
          </div>
        </div>

      </div>

